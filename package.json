{
  "name": "react-wizard",
  "version": "1.0.0",
  "description": "assignment",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
    "dev": "parcel src/index.html",
